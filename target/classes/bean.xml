<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/aop 
	http://www.springframework.org/schema/aop/spring-aop-3.0.xsd ">


	<!-- metier -->
	<bean name="produit"
		class="sn.ucad.gestionCommerciale.metier.Produit"></bean> <!-- Bean produit -->
	<bean name="categorie"
		class="sn.ucad.gestionCommerciale.metier.Categorie"></bean> <!-- Bean categorie -->
	<bean name="format" 
		class="sn.ucad.gestionCommerciale.metier.Format"></bean> <!-- Bean format -->
	<bean name="conditionnement"
		class="sn.ucad.gestionCommerciale.metier.Conditionnement"></bean> <!-- Bean conditionnement -->
	<bean name="marque"
		class="sn.ucad.gestionCommerciale.metier.Marque"></bean> <!-- Bean marque -->


	<!-- Dao -->
	<bean name="commercantDao" 
		class="sn.ucad.gestionCommerciale.dao.CommercantDao"></bean> <!-- Bean CommercantDao -->


	<!-- service -->
	<bean name="commercantService" id="commercantService"
		class="sn.ucad.gestionCommerciale.service.CommercantService"></bean> <!-- Bean CommercantService -->


</beans>